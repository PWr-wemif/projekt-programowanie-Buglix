{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Qt Project",
        "type": "cppdbg",
        "request": "launch",
        "program": "${EiT 3 SEMESTR}/_repos/to/your/compiled/executable", // Ścieżka do pliku wykonywalnego
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "preLaunchTask": "build-qt-project" // Zadanie kompilacji zdefiniowane w tasks.json
      }
    ]
  }
  